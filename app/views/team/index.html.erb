<% content_for :title, "Teams" %>
<div class="panel panel-primary text-center">
  <div class="panel-heading">
    <h1>Teams</h1>
  </div>
  <div class="panel-body">
    <ul class="list-group">
      <% @teams.each do |team| %>
        <li class="list-group-item"><%= link_to team.name, { controller: 'team', action: 'show', id: team.id } %></li>
      <% end %>
    </ul>
  </div>
</div>
