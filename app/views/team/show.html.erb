<% content_for :title, @team.name %>
<div class="panel panel-primary text-center">
  <div class="panel-heading">
    <h1>Teams</h1>
  </div>
  <div class="panel-body">
    <ul class="list-group">
      <% @games.each do |game| %>
        <div style="border-bottom: 20px solid black;">
          <img src="https://s3.amazonaws.com/mlb-daemon/images/<%= game.id %>.png" />
        </div>
      <% end %>
    </ul>
  </div>
</div>
